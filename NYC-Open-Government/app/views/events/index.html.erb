<h1>Timeline</h1>
<% @events.each do |event| %>
  <li>
    <%= event.name %>
    <%= event.category %>
    <%= event.description %>
    <%= event.status %>
    <%= event.url %>
    <%= link_to 'Show', event_path(event) %>
    <%= link_to 'Edit', edit_event_path(event) %>
    <%= link_to 'Destroy', event, confirm: 'Are you sure?', method: :delete %>
  </li>
<% end %>
